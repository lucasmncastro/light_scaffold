<h1>Detalhes do <%= object_label %></h1>

<dl>
<%- for column in show_columns -%>
  <dd>
    <%= model.human_attribute_name column %>
  </dd>
  <dt>
    <%= show_column column, object %>
  </dt>
<%- end -%>
</dl>

<p>
<%= link_to 'Editar', edit_object_url %>
|
<%= link_to 'Voltar', collection_url %>
</p>
